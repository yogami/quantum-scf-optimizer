{
  "name": "instagram-reel-poster",
  "version": "1.0.0",
  "description": "Production-ready backend service for Instagram reels generation for Challenging View",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:unit": "jest --testPathPatterns=tests/unit",
    "test:integration": "jest --testPathPatterns=tests/integration",
    "test:e2e": "jest --testPathPatterns=tests/e2e",
    "test:parity": "jest tests/e2e/ProductionParity.test.ts --runInBand",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --coverageThreshold='{\"global\":{\"lines\":80,\"branches\":80}}'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "lint:strict": "eslint src/**/*.ts --max-warnings=0",
    "lint:duplication": "jscpd src --threshold 5",
    "quality": "npm run lint:strict && npm run lint:duplication && npm run test:ci",
    "format": "prettier --write src/**/*.ts tests/**/*.ts"
  },
  "keywords": [
    "instagram",
    "reels",
    "tts",
    "ai",
    "video-generation"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/qrcode": "^1.5.6",
    "axios": "^1.13.2",
    "bullmq": "^5.66.4",
    "cheerio": "^1.1.2",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.5",
    "ioredis": "^5.8.2",
    "playwright": "^1.57.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "replicate": "^1.4.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "jscpd": "^4.0.5",
    "nock": "^14.0.10",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
